rule touch
    command = cmd /c type nul > "$out"
    description = TOUCH $out

rule compdb
  command = cmd /c "ninja -C $dir -t compdb > $out"
  description = COMPILE DB
  pool = console
  restat = 1

rule meta
  command = $builddir/meta.exe $flags $in -o $out -- $cflags
  description = META $in
  restat = 1
